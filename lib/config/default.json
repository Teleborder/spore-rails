{
  "host": "https://pod.spore.sh",
  "useProxy": true,
  "proxy": {
    "home": "~/.spore/spored",
    "port": 8380,
    "errorFile": "~/.spore/spored/errors.log",
    "logFile": "~/.spore/spored/log.log"
